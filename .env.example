# Supabase Keys
VITE_SUPABASE_ANON_KEY=
VITE_SUPABASE_URL=https://[YOUR SUPABASE PROJECT ID].supabase.co/
VITE_SUPABASE_SERVICE_ROLE_KEY=

## AI Keys
OPENAI_API_KEY=

## Stripe live
STRIPE_SECRET_LIVE_KEY=
STRIPE_LIVE_WEBHOOK_SECRET=

## Stripe test
VITE_STRIPE_TEST_MODE=true ## Set to "false" when you want Stripe to actually run transactions
STRIPE_SECRET_TEST_KEY=
STRIPE_TEST_WEBHOOK_SECRET=

## Analytics
VITE_GA_MEASUREMENT_ID= ## your Google Analytics ID (e.g., G-XXXXXXXXXX)

## Customer Support
VITE_CHATWOOT_BASE_URL= ## e.g., https://app.chatwoot.com
VITE_CHATWOOT_WEBSITE_TOKEN= ## Your Chatwoot Website Inbox Token

## Email Marketing
EMAIL_MARKETING_PROVIDER=kit ## Provider name (e.g., 'kit', 'mailerlite', leave empty for dummy)
EMAIL_MARKETING_BASE_URL=https://api.kit.com ## Base URL for the provider's V4 API
EMAIL_MARKETING_API_KEY= ## API Key for the selected provider
EMAIL_MARKETING_TAG_ID= ## Required Tag ID for adding users via Kit
# EMAIL_MARKETING_FORM_ID= ## Optional: Specific Form ID if needed by provider

# Analytics (Optional)
# Set VITE_ANALYTICS_PROVIDER to 'posthog' (or 'mixpanel' in future) to enable.
# If provider is set, the corresponding API key is required.
VITE_ANALYTICS_PROVIDER="none" # Options: "none", "posthog"
VITE_POSTHOG_KEY=""            # Required if provider is "posthog"
VITE_POSTHOG_HOST="https://app.posthog.com" # Optional: Defaults to PostHog cloud